package prueba.Algoritmos;

public class Pruebas {

	public static void main(String[] args) {
		
		Ensayo nuevo01 = new Ensayo();
		
		System.out.print(nuevo01.texto("Hola perro estamos probando esta monda"));
		

	}

}


class Ensayo{
	
	public char[] texto(String v) {
		
		return v.charAt(v);
	}
	
}
